<div class='page-header'>
	<h1>Add Mem</h1>
</div>

<%= form_for(:mem, url:{action:'create', id:@mem.id}, method: :post ) do |field| %>
	<%= render(partial: 'form', locals:{mem: field})%>
	<%= link_to('Back', {action: 'index'}, class: 'btn btn-warning')%>
	<%= submit_tag('Create', class:'btn btn-primary')%>
<%end%>