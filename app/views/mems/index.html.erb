<div class='page-header'>
	<h1>Mems</h1>
</div>
<table>
<% @mem.each do |mem|%>

	<tr class='center'><%= mem.name%></tr><br>
	<tr class='center'><%= mem.description%></tr><br>
	<tr class='center'><%= link_to(image_tag(mem.image),{action:'show', id:mem.id}) %></tr><br>
	
<p class='actions'>
<%= link_to('Back', {action:'index'}, class:'btn btn-primary')%>
<%= link_to('Edit', {action:'edit', id:mem.id}, class: 'btn btn-primary')%>
<%= link_to('Delete', {action: 'delete', id: mem.id}, class: 'btn btn-danger') %>
</p>
	
<%end%>
</table>
